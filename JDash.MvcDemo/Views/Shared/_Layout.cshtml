<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - JDash MVC Demo</title>
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-switch.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-colorselector.css" rel="stylesheet" />
    <link href="~/Content/css/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    <link href="~/Content/css/Site.css" rel="stylesheet" />
</head>
<body>




    <!-- Fixed navbar -->
    <div class="navbar navbar-inverse appnavbar navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">JDash Asp.Net Mvc</a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="http://www.jdash.net/" target="_blank">Home</a>
                    </li>
                    <li>
                        <a href="http://forum.jdash.net" target="_blank">Support</a>
                    </li>
                    <li>
                        <a href="http://www.jdash.net/purchase" target="_blank">Purchase</a>
                    </li>
                    <li>
                        <a href="http://www.jdash.net/contact" target="_blank">Contact</a>
                    </li>
                </ul>

                <ul class="nav navbar-nav navbar-right">


                    <li class="dropdown">

                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Change Theme<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            @foreach (var item in Html.JDash().ResourceManager().Themes)
                            {
                                <li>
                                    @Html.JDash().ThemeChangeLink().Theme(item.Name).InnerHtml(item.Name).Render()
                                </li>
                            }
                        </ul>

                    </li>

                    <li>
                        @(Html.JDash().ThemeStylesList().Render())
                    </li>

                </ul>
            </div>
            <!--/.nav-collapse -->
        </div>
    </div>

    <!-- Begin page content -->
    <div class="container-fluid">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/sly")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/swatch")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/history")




    @(Html.JDash().ResourceManager()
        .Theme("flat")
        .Style("w")
        .CookieForTheme(true)
        .ClientInitHandler("window.runApp && window.runApp();")
        .Render())

    @RenderSection("script", false)

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-39892625-3', 'jdash.net');
        ga('send', 'pageview');

    </script>

</body>
</html>
